      <JFXButton fx:id="backBtn" buttonType="RAISED" layoutX="650.0" layoutY="477.0" onAction="#handleBack" text="Back" />
      <JFXDatePicker fx:id="startDate" defaultColor="#435a9a" layoutX="538.0" layoutY="159.0" promptText="Start" />
      <JFXDatePicker fx:id="endDate" defaultColor="#435a9a" layoutX="538.0" layoutY="234.0" promptText="End" />
